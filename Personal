            Me.ABPriorityLabel18.ForeColor = RGB(255, 0, 0) 'red
        End If
        ABSuccCrit18.Enabled = ABResolve18.Value
        ABWIR18.Enabled = ABResolve18.Value
        ABWho18.Enabled = ABResolve18.Value
        ABAPA18.Enabled = ABResolve18.Value
        ABAction18.Enabled = ABResolve18.Value
        ABWID18.Enabled = ABResolve18.Value
        ABWhen18.Enabled = ABResolve18.Value
        ABAS18.Enabled = ABResolve18.Value
        ABAdviserScore18.Enabled = ABResolve18.Value
            If ABAdviserScore18.Enabled = False Then
                ABAdviserScore18.Value = 0
            ElseIf ABStatus18.Value = "Not Started" Then
                ABAdviserScore6 = 20
            ElseIf ABStatus18.Value = "Stalled" Then
                ABAdviserScore18.Value = 40
            ElseIf ABStatus18.Value = "In Progress" Then
                ABAdviserScore18.Value = 60
            ElseIf ABStatus18.Value = "Partial" Then
                ABAdviserScore18.Value = 80
            ElseIf ABStatus18.Value = "Completed" Then
                ABAdviserScore18.Value = 100
            End If
        ABASP18.Enabled = ABResolve18.Value
        ABBE18.Enabled = ABResolve18.Value
        ABBEScore18.Enabled = ABResolve18.Value
            If ABBEScore18.Enabled = False Then
                ABBEScore18.Value = 0
            ElseIf ABBEScore18.Enabled = True Then
                If ABFacilitated18.Value = True Then
                    ABBEScore18.Value = ABBEScore18.Value + 20
                End If
                If ABInformCont18.Value = True Then
                    ABBEScore18.Value = ABBEScore18.Value + 20
                End If
                If ABAdvised18.Value = True Then
                    ABBEScore18.Value = ABBEScore18.Value + 20
                End If
                If ABMetexpect18.Value = True Then
                    ABBEScore18.Value = ABBEScore18.Value + 20
                End If
                If ABChangeOcc18.Value = True Then
                    ABBEScore18.Value = ABBEScore18.Value + 20
                End If
            End If
        ABBESP18.Enabled = ABResolve18.Value
        ABTS18.Enabled = ABResolve18.Value
        ABTotalScore18.Enabled = ABResolve18.Value
            If ABTotalScore18.Enabled = False Then
                    ABTotalScore18.Value = 0
                ElseIf ABTotalScore18.Enabled = True Then
                    ABTotalScore18 = (ABAdviserScore18.Value / 2) + (ABBEScore18.Value / 2)
            End If
        ABTSP18.Enabled = ABResolve18.Value
        ABStatus18.Enabled = ABResolve18.Value
        ABFacilitated18.Enabled = ABResolve18.Value
        ABInformCont18.Enabled = ABResolve18.Value
        ABAdvised18.Enabled = ABResolve18.Value
        ABMetexpect18.Enabled = ABResolve18.Value
        ABChangeOcc18.Enabled = ABResolve18.Value
End Sub
