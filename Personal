Private Sub ModifyTableRow(TableRow As Range)
    
    Set LUYBLTable = Worksheets("LUYBL").ListObjects("LUYBL")
    
    With TableRow
    
            .Cells(1, 1) = Branch.Value
            .Cells(1, 2) = FirstName.Value
            .Cells(1, 3) = Surname.Value
            .Cells(1, 4) = Cellphone.Value
            .Cells(1, 5) = Tenure.Value
'Section 1
            .Cells(1, 6) = ABResponse1.Value
            .Cells(1, 7) = ABResolve1.Value
                If ABRP1.Value = True Then 'Store the radio button value according to selection
                    .Cells(1, 8) = "1"
                End If
                If ABRP2.Value = True Then
                    .Cells(1, 8) = "2"
                End If
                If ABRP3.Value = True Then
                    .Cells(1, 8) = "3"
                End If
                If ABRp4.Value = True Then
                    .Cells(1, 8) = "4"
                End If
                If ABRP5.Value = True Then
                    .Cells(1, 8) = "5"
                End If
            .Cells(1, 9) = ABWho1.Value
            .Cells(1, 10) = ABAction1.Value
            .Cells(1, 11) = ABWhen1.Value
            .Cells(1, 12) = ABStatus1.Value
            .Cells(1, 13) = ABFacilitated1.Value
            .Cells(1, 14) = ABInformCont1.Value
            .Cells(1, 15) = ABAdvised1.Value
            .Cells(1, 16) = ABMetexpect1.Value
            .Cells(1, 17) = ABChangeOcc1.Value
            .Cells(1, 18) = ABAdviserScore1.Value
            .Cells(1, 19) = ABBEScore1.Value
            .Cells(1, 20) = ABTotalScore1.Value
'Section 2
            .Cells(1, 21) = ABResponse2.Value
            .Cells(1, 22) = ABResolve2.Value
                If ABRP6.Value = True Then 'Store the radio button value according to selection
                    .Cells(1, 23) = "1"
