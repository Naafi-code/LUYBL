            ElseIf ABStatus19.Value = "Partial" Then
                ABAdviserScore19.Value = 80
            ElseIf ABStatus19.Value = "Completed" Then
                ABAdviserScore19.Value = 100
            End If
        ABASP19.Enabled = ABResolve19.Value
        ABBE19.Enabled = ABResolve19.Value
        'Set the score values to 0 or the actual value if recommend is true or false
        ABBEScore19.Enabled = ABResolve19.Value
            If ABBEScore19.Enabled = False Then
                ABBEScore19.Value = 0
            ElseIf ABBEScore19.Enabled = True Then
                ABBEScore19.Value = .Cells(1, 289).Value
            End If
        ABBESP19.Enabled = ABResolve19.Value
        ABTS19.Enabled = ABResolve19.Value
        'Set the score values to 0 or the actual value if recommend is true or false
        ABTotalScore19.Enabled = ABResolve19.Value
            If ABTotalScore19.Enabled = False Then
                    ABTotalScore19.Value = 0
                ElseIf ABTotalScore19.Enabled = True Then
                    ABTotalScore19 = (ABAdviserScore19.Value / 2) + (ABBEScore19.Value / 2)
            End If
        ABTSP19.Enabled = ABResolve19.Value
        ABStatus19.Enabled = ABResolve19.Value
        ABFacilitated19.Enabled = ABResolve19.Value
        ABInformCont19.Enabled = ABResolve19.Value
        ABAdvised19.Enabled = ABResolve19.Value
        ABMetexpect19.Enabled = ABResolve19.Value
        ABChangeOcc19.Enabled = ABResolve19.Value
                
        End With
        
End Sub
