            If ABTotalScore24.Enabled = False Then
                    ABTotalScore24.Value = 0
                ElseIf ABTotalScore24.Enabled = True Then
                    ABTotalScore24 = (ABAdviserScore24.Value / 2) + (ABBEScore24.Value / 2)
            End If
        ABTSP24.Enabled = ABResolve24.Value
        ABStatus24.Enabled = ABResolve24.Value
        ABFacilitated24.Enabled = ABResolve24.Value
        ABInformCont24.Enabled = ABResolve24.Value
        ABAdvised24.Enabled = ABResolve24.Value
        ABMetexpect24.Enabled = ABResolve24.Value
        ABChangeOcc24.Enabled = ABResolve24.Value
                
        End With
        
End Sub
Private Sub Section25(SelectedRow As Range)

   Set LUYBLTable = Worksheets("LUYBL").ListObjects("LUYBL")
    
   With SelectedRow

        ABResponse25.Value = .Cells(1, 366).Value
        ABResolve25.Value = .Cells(1, 367).Value
        ABPriority25.Value = .Cells(1, 368).Value 'Get the existing value of the priority radio button
                If ABPriority25.Value = 1 Then
                    ABRP121.Value = True
                Else
                    ABRP121.Value = False
                End If
                If ABPriority25.Value = 2 Then
                    ABRP122.Value = True
                Else
                    ABRP122.Value = False
                End If
                If ABPriority25.Value = 3 Then
                    ABRP123.Value = True
                Else
                    ABRP123.Value = False
                End If
                If ABPriority25.Value = 4 Then
                    ABRP124.Value = True
                Else
                    ABRP124.Value = False
                End If
                If ABPriority25.Value = 5 Then
                    ABRP125.Value = True
                Else
                    ABRP125.Value = False
                End If
        ABWho25.Value = .Cells(1, 369).Value
        ABAction25.Value = .Cells(1, 370).Value
        ABWhen25.Value = .Cells(1, 371).Value
        ABStatus25.Value = .Cells(1, 372).Value
        ABFacilitated25.Value = .Cells(1, 373).Value
        ABInformCont25.Value = .Cells(1, 374).Value
        ABAdvised25.Value = .Cells(1, 375).Value
        ABMetexpect25.Value = .Cells(1, 376).Value
        ABChangeOcc25.Value = .Cells(1, 377).Value
        ABAdviserScore25.Value = .Cells(1, 378).Value
            If ABStatus25.Value = "Not Started" Then
                ABAdviserScore25 = 20
            End If
        ABBEScore25.Value = .Cells(1, 379).Value
        ABTotalScore25.Value = .Cells(1, 380).Value
        'Set the visibilty value for radio buttons based on Resolve tick box status true = enabled false = not enabled
        ABWhat25.Enabled = ABResolve25.Value
        ABRecommend25.Enabled = ABResolve25.Value
        ABRP121.Visible = ABResolve25.Value
        ABRP122.Visible = ABResolve25.Value
        ABRP123.Visible = ABResolve25.Value
        ABRP124.Visible = ABResolve25.Value
        ABRP125.Visible = ABResolve25.Value
        ABPriority25.Visible = ABResolve25.Value
        'Set the enabled status per item if recommendation is selected or not, as per resolve tick box
            If ABResolve25.Value = True Then
                ABPriorityLabel25.Caption = "Priority"
                Me.ABPriorityLabel25.ForeColor = RGB(0, 128, 0) 'green
            End If
            If ABResolve25.Value = False Then
                ABPriorityLabel25.Caption = "Not selected for remediation."
                Me.ABPriorityLabel25.ForeColor = RGB(255, 0, 0) 'red
            End If
        ABSuccCrit25.Enabled = ABResolve25.Value
        ABWIR25.Enabled = ABResolve25.Value
        ABWho25.Enabled = ABResolve25.Value
        ABAPA25.Enabled = ABResolve25.Value
        ABAction25.Enabled = ABResolve25.Value
        ABWID25.Enabled = ABResolve25.Value
        ABWhen25.Enabled = ABResolve25.Value
        ABAS25.Enabled = ABResolve25.Value
        ABAdviserScore25.Enabled = ABResolve25.Value
            If ABAdviserScore25.Enabled = False Then
                ABAdviserScore25 = 0
            ElseIf ABStatus25.Value = "Not Started" Then
                ABAdviserScore25.Value = 20
            ElseIf ABStatus25.Value = "Stalled" Then
               ABAdviserScore25.Value = 40
            ElseIf ABStatus25.Value = "In Progress" Then
                ABAdviserScore25.Value = 60
            ElseIf ABStatus25.Value = "Partial" Then
                ABAdviserScore25.Value = 80
            ElseIf ABStatus25.Value = "Completed" Then
                ABAdviserScore25.Value = 100
            End If
        ABASP25.Enabled = ABResolve25.Value
        ABBE25.Enabled = ABResolve25.Value
        'Set the score values to 0 or the actual value if recommend is true or false
        ABBEScore25.Enabled = ABResolve25.Value
            If ABBEScore25.Enabled = False Then
                ABBEScore25.Value = 0
            ElseIf ABBEScore25.Enabled = True Then
                ABBEScore25.Value = .Cells(1, 379).Value
            End If
        ABBESP25.Enabled = ABResolve25.Value
        ABTS25.Enabled = ABResolve25.Value
        'Set the score values to 0 or the actual value if recommend is true or false
        ABTotalScore25.Enabled = ABResolve25.Value
            If ABTotalScore25.Enabled = False Then
                    ABTotalScore25.Value = 0
                ElseIf ABTotalScore25.Enabled = True Then
                    ABTotalScore25 = (ABAdviserScore25.Value / 2) + (ABBEScore25.Value / 2)
            End If
        ABTSP25.Enabled = ABResolve25.Value
        ABStatus25.Enabled = ABResolve25.Value
        ABFacilitated25.Enabled = ABResolve25.Value
        ABInformCont25.Enabled = ABResolve25.Value
        ABAdvised25.Enabled = ABResolve25.Value
        ABMetexpect25.Enabled = ABResolve25.Value
        ABChangeOcc25.Enabled = ABResolve25.Value
                
        End With
        
End Sub
