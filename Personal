            End If
        ABBEScore9.Value = .Cells(1, 139).Value
        ABTotalScore9.Value = .Cells(1, 140).Value
        'Set the visibilty value for radio buttons based on Resolve tick box status true = enabled false = not enabled
        ABWhat9.Enabled = ABResolve9.Value
        ABRecommend9.Enabled = ABResolve9.Value
        ABRP41.Visible = ABResolve9.Value
        ABRP42.Visible = ABResolve9.Value
        ABRP43.Visible = ABResolve9.Value
        ABRP44.Visible = ABResolve9.Value
        ABRP45.Visible = ABResolve9.Value
        ABPriority9.Visible = ABResolve9.Value
        'Set the enabled status per item if recommendation is selected or not, as per resolve tick box
            If ABResolve9.Value = True Then
                ABPriorityLabel9.Caption = "Priority"
                Me.ABPriorityLabel9.ForeColor = RGB(0, 128, 0) 'green
            End If
            If ABResolve9.Value = False Then
                ABPriorityLabel9.Caption = "Not selected for remediation."
                Me.ABPriorityLabel9.ForeColor = RGB(255, 0, 0) 'red
            End If
        ABSuccCrit9.Enabled = ABResolve9.Value
        ABWIR9.Enabled = ABResolve9.Value
        ABWho9.Enabled = ABResolve9.Value
        ABAPA9.Enabled = ABResolve9.Value
        ABAction9.Enabled = ABResolve9.Value
        ABWID9.Enabled = ABResolve9.Value
        ABWhen9.Enabled = ABResolve9.Value
        ABAS9.Enabled = ABResolve9.Value
        ABAdviserScore9.Enabled = ABResolve9.Value
            If ABAdviserScore9.Enabled = False Then
                ABAdviserScore9 = 0
            ElseIf ABStatus9.Value = "Not Started" Then
                ABAdviserScore9.Value = 20
            ElseIf ABStatus9.Value = "Stalled" Then
               ABAdviserScore9.Value = 40
            ElseIf ABStatus9.Value = "In Progress" Then
                ABAdviserScore9.Value = 60
            ElseIf ABStatus9.Value = "Partial" Then
                ABAdviserScore9.Value = 80
            ElseIf ABStatus9.Value = "Completed" Then
                ABAdviserScore9.Value = 100
            End If
        ABASP9.Enabled = ABResolve9.Value
        ABBE9.Enabled = ABResolve9.Value
        'Set the score values to 0 or the actual value if recommend is true or false
        ABBEScore9.Enabled = ABResolve9.Value
            If ABBEScore9.Enabled = False Then
                ABBEScore9.Value = 0
            ElseIf ABBEScore9.Enabled = True Then
                ABBEScore9.Value = .Cells(1, 139).Value
            End If
        ABBESP9.Enabled = ABResolve9.Value
        ABTS9.Enabled = ABResolve9.Value
        'Set the score values to 0 or the actual value if recommend is true or false
        ABTotalScore9.Enabled = ABResolve9.Value
            If ABTotalScore9.Enabled = False Then
                    ABTotalScore9.Value = 0
                ElseIf ABTotalScore9.Enabled = True Then
                    ABTotalScore9 = (ABAdviserScore9.Value / 2) + (ABBEScore9.Value / 2)
            End If
        ABTSP9.Enabled = ABResolve9.Value
        ABStatus9.Enabled = ABResolve9.Value
        ABFacilitated9.Enabled = ABResolve9.Value
        ABInformCont9.Enabled = ABResolve9.Value
        ABAdvised9.Enabled = ABResolve9.Value
        ABMetexpect9.Enabled = ABResolve9.Value
        ABChangeOcc9.Enabled = ABResolve9.Value
                
        End With
        
End Sub
