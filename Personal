            ElseIf ABStatus25.Value = "Not Started" Then
                ABAdviserScore6 = 20
            ElseIf ABStatus25.Value = "Stalled" Then
                ABAdviserScore25.Value = 40
            ElseIf ABStatus25.Value = "In Progress" Then
                ABAdviserScore25.Value = 60
            ElseIf ABStatus25.Value = "Partial" Then
                ABAdviserScore25.Value = 80
            ElseIf ABStatus25.Value = "Completed" Then
                ABAdviserScore25.Value = 100
            End If
        ABASP25.Enabled = ABResolve25.Value
        ABBE25.Enabled = ABResolve25.Value
        ABBEScore25.Enabled = ABResolve25.Value
            If ABBEScore25.Enabled = False Then
                ABBEScore25.Value = 0
            ElseIf ABBEScore25.Enabled = True Then
                If ABFacilitated25.Value = True Then
                    ABBEScore25.Value = ABBEScore25.Value + 20
                End If
                If ABInformCont25.Value = True Then
                    ABBEScore25.Value = ABBEScore25.Value + 20
                End If
                If ABAdvised25.Value = True Then
                    ABBEScore25.Value = ABBEScore25.Value + 20
                End If
                If ABMetexpect25.Value = True Then
                    ABBEScore25.Value = ABBEScore25.Value + 20
                End If
                If ABChangeOcc25.Value = True Then
                    ABBEScore25.Value = ABBEScore25.Value + 20
                End If
            End If
        ABBESP25.Enabled = ABResolve25.Value
        ABTS25.Enabled = ABResolve25.Value
        ABTotalScore25.Enabled = ABResolve25.Value
            If ABTotalScore25.Enabled = False Then
                    ABTotalScore25.Value = 0
                ElseIf ABTotalScore25.Enabled = True Then
                    ABTotalScore25 = (ABAdviserScore25.Value / 2) + (ABBEScore25.Value / 2)
            End If
        ABTSP25.Enabled = ABResolve25.Value
        ABStatus25.Enabled = ABResolve25.Value
        ABFacilitated25.Enabled = ABResolve25.Value
        ABInformCont25.Enabled = ABResolve25.Value
        ABAdvised25.Enabled = ABResolve25.Value
        ABMetexpect25.Enabled = ABResolve25.Value
        ABChangeOcc25.Enabled = ABResolve25.Value
End Sub
Private Sub ABResolve26_Click()
'Hide or show the recommendation text boxs and priorities if a recommendation must be made.
'Section 26
        ABWhat26.Enabled = ABResolve26.Value
        ABRecommend26.Enabled = ABResolve26.Value
        ABRP126.Visible = ABResolve26.Value
        ABRP127.Visible = ABResolve26.Value
        ABRP128.Visible = ABResolve26.Value
        ABRP129.Visible = ABResolve26.Value
        ABRP130.Visible = ABResolve26.Value
        ABPriority26.Visible = ABResolve26.Value
'       Set the status per recommendation if selected or not to resolve1
        If ABResolve26.Value = True Then
            ABPriorityLabel26.Caption = "Priority"
            Me.ABPriorityLabel26.ForeColor = RGB(0, 128, 0) 'green
        End If
        If ABResolve26.Value = False Then
            ABPriorityLabel26.Caption = "Not selected for remediation."
            Me.ABPriorityLabel26.ForeColor = RGB(255, 0, 0) 'red
        End If
        ABSuccCrit26.Enabled = ABResolve26.Value
        ABWIR26.Enabled = ABResolve26.Value
        ABWho26.Enabled = ABResolve26.Value
        ABAPA26.Enabled = ABResolve26.Value
        ABAction26.Enabled = ABResolve26.Value
        ABWID26.Enabled = ABResolve26.Value
        ABWhen26.Enabled = ABResolve26.Value
        ABAS26.Enabled = ABResolve26.Value
        ABAdviserScore26.Enabled = ABResolve26.Value
            If ABAdviserScore26.Enabled = False Then
                ABAdviserScore26.Value = 0
            ElseIf ABStatus26.Value = "Not Started" Then
                ABAdviserScore6 = 20
            ElseIf ABStatus26.Value = "Stalled" Then
                ABAdviserScore26.Value = 40
            ElseIf ABStatus26.Value = "In Progress" Then
                ABAdviserScore26.Value = 60
            ElseIf ABStatus26.Value = "Partial" Then
                ABAdviserScore26.Value = 80
            ElseIf ABStatus26.Value = "Completed" Then
                ABAdviserScore26.Value = 100
            End If
        ABASP26.Enabled = ABResolve26.Value
        ABBE26.Enabled = ABResolve26.Value
        ABBEScore26.Enabled = ABResolve26.Value
            If ABBEScore26.Enabled = False Then
                ABBEScore26.Value = 0
            ElseIf ABBEScore26.Enabled = True Then
                If ABFacilitated26.Value = True Then
                    ABBEScore26.Value = ABBEScore26.Value + 20
                End If
                If ABInformCont26.Value = True Then
                    ABBEScore26.Value = ABBEScore26.Value + 20
                End If
                If ABAdvised26.Value = True Then
                    ABBEScore26.Value = ABBEScore26.Value + 20
                End If
                If ABMetexpect26.Value = True Then
                    ABBEScore26.Value = ABBEScore26.Value + 20
                End If
                If ABChangeOcc26.Value = True Then
                    ABBEScore26.Value = ABBEScore26.Value + 20
                End If
            End If
        ABBESP26.Enabled = ABResolve26.Value
        ABTS26.Enabled = ABResolve26.Value
        ABTotalScore26.Enabled = ABResolve26.Value
            If ABTotalScore26.Enabled = False Then
                    ABTotalScore26.Value = 0
                ElseIf ABTotalScore26.Enabled = True Then
                    ABTotalScore26 = (ABAdviserScore26.Value / 2) + (ABBEScore26.Value / 2)
            End If
        ABTSP26.Enabled = ABResolve26.Value
        ABStatus26.Enabled = ABResolve26.Value
        ABFacilitated26.Enabled = ABResolve26.Value
        ABInformCont26.Enabled = ABResolve26.Value
        ABAdvised26.Enabled = ABResolve26.Value
        ABMetexpect26.Enabled = ABResolve26.Value
        ABChangeOcc26.Enabled = ABResolve26.Value
End Sub
