End Sub
Sub PopulateForm()
        
        Call Section1
        Call Section2
        Call Section3
        Call Section4
        Call Section5
        Call Section6
        Call Section7
        Call Section8
        Call Section9
        Call Section10
        Call Section11
        Call Section12
        Call Section13
        Call Section14
        Call Section15
        Call Section16
        Call Section17
        Call Section18
        Call Section19
        Call Section20
        Call Section21
        Call Section22
        Call Section23
        Call Section24
        Call Section25
        Call Section26
        
End Sub
    
Private Sub Section1(SelectedRow As Range)

   Set LUYBLTable = Worksheets("LUYBL").ListObjects("LUYBL")
    
   With SelectedRow
   
   'Call the common data to the top of the form
        Branch.Value = .Cells(1, 1).Value
        FirstName.Value = .Cells(1, 2).Value
        Surname.Value = .Cells(1, 3).Value
        Cellphone.Value = .Cells(1, 4).Value
        Tenure.Value = .Cells(1, 5).Value
        ABToolsText.Value = .Cells(1, 396).Value
        ABNotesText.Value = .Cells(1, 397).Value
        

        ABResponse1.Value = .Cells(1, 6).Value
        ABResolve1.Value = .Cells(1, 7).Value
        ABPriority1.Value = .Cells(1, 8).Value 'Get the existing value of the priority radio button
                If ABPriority1.Value = 1 Then
                    ABRP1.Value = True
                Else
                   ABRP1.Value = False
                End If
                If ABPriority1.Value = 2 Then
                    ABRP2.Value = True
                Else
                    ABRP2.Value = False
                End If
                If ABPriority1.Value = 3 Then
                    ABRP3.Value = True
                Else
                    ABRP3.Value = False
                End If
                If ABPriority1.Value = 4 Then
                    ABRp4.Value = True
                Else
                    ABRp4.Value = False
                End If
                If ABPriority1.Value = 5 Then
                    ABRP5.Value = True
                Else
                    ABRP5.Value = False
                End If
        ABWho1.Value = .Cells(1, 9).Value
        ABAction1.Value = .Cells(1, 10).Value
        ABWhen1.Value = .Cells(1, 11).Value
        ABStatus1.Value = .Cells(1, 12).Value
        ABFacilitated1.Value = .Cells(1, 13).Value
        ABInformCont1.Value = .Cells(1, 14).Value
        ABAdvised1.Value = .Cells(1, 15).Value
        ABMetexpect1.Value = .Cells(1, 16).Value
        ABChangeOcc1.Value = .Cells(1, 17).Value
        ABAdviserScore1.Value = .Cells(1, 18).Value
            If ABStatus1.Value = "Not Started" Then
                ABAdviserScore1 = 20
            End If
        ABBEScore1.Value = .Cells(1, 19).Value
        ABTotalScore1.Value = .Cells(1, 20).Value
        'Set the visibilty value for radio buttons based on Resolve tick box status true = enabled false = not enabled
        ABWhat1.Enabled = ABResolve1.Value
        ABRecommend1.Enabled = ABResolve1.Value
        ABRP1.Visible = ABResolve1.Value
        ABRP2.Visible = ABResolve1.Value
        ABRP3.Visible = ABResolve1.Value
        ABRp4.Visible = ABResolve1.Value
        ABRP5.Visible = ABResolve1.Value
        ABPriority1.Visible = ABResolve1.Value
        'Set the enabled status per item if recommendation is selected or not, as per resolve tick box
            If ABResolve1.Value = True Then
                ABPriorityLabel1.Caption = "Priority"
                Me.ABPriorityLabel1.ForeColor = RGB(0, 128, 0) 'green
            End If
            If ABResolve1.Value = False Then
                ABPriorityLabel1.Caption = "Not selected for remediation."
                Me.ABPriorityLabel1.ForeColor = RGB(255, 0, 0) 'red
            End If
        ABSuccCrit1.Enabled = ABResolve1.Value
        ABWIR1.Enabled = ABResolve1.Value
        ABWho1.Enabled = ABResolve1.Value
        ABAPA1.Enabled = ABResolve1.Value
        ABAction1.Enabled = ABResolve1.Value
        ABWID1.Enabled = ABResolve1.Value
        ABWhen1.Enabled = ABResolve1.Value
        ABAS1.Enabled = ABResolve1.Value
        ABAdviserScore1.Enabled = ABResolve1.Value
            If ABAdviserScore1.Enabled = False Then
                ABAdviserScore1 = 0
            ElseIf ABStatus1.Value = "Not Started" Then
                ABAdviserScore1.Value = 20
            ElseIf ABStatus1.Value = "Stalled" Then
               ABAdviserScore1.Value = 40
            ElseIf ABStatus1.Value = "In Progress" Then
                ABAdviserScore1.Value = 60
            ElseIf ABStatus1.Value = "Partial" Then
                ABAdviserScore1.Value = 80
            ElseIf ABStatus1.Value = "Completed" Then
                ABAdviserScore1.Value = 100
            End If
        ABASP1.Enabled = ABResolve1.Value
        ABBE1.Enabled = ABResolve1.Value
        'Set the score values to 0 or the actual value if recommend is true or false
        ABBEScore1.Enabled = ABResolve1.Value
            If ABBEScore1.Enabled = False Then
                ABBEScore1.Value = 0
            ElseIf ABBEScore1.Enabled = True Then
                ABBEScore1.Value = .Cells(1, 19).Value
            End If
        ABBESP1.Enabled = ABResolve1.Value
        ABTS1.Enabled = ABResolve1.Value
        'Set the score values to 0 or the actual value if recommend is true or false
        ABTotalScore1.Enabled = ABResolve1.Value
            If ABTotalScore1.Enabled = False Then
                    ABTotalScore1.Value = 0
                ElseIf ABTotalScore1.Enabled = True Then
                    ABTotalScore1 = (ABAdviserScore1.Value / 2) + (ABBEScore1.Value / 2)
            End If
        ABTSP1.Enabled = ABResolve1.Value
        ABStatus1.Enabled = ABResolve1.Value
        ABFacilitated1.Enabled = ABResolve1.Value
        ABInformCont1.Enabled = ABResolve1.Value
        ABAdvised1.Enabled = ABResolve1.Value
        ABMetexpect1.Enabled = ABResolve1.Value
        ABChangeOcc1.Enabled = ABResolve1.Value
        
    End With
        
End Sub
Private Sub Section2(SelectedRow As Range)

   Set LUYBLTable = Worksheets("LUYBL").ListObjects("LUYBL")
    
   With SelectedRow

        ABResponse2.Value = .Cells(1, 21).Value
        ABResolve2.Value = .Cells(1, 22).Value
        ABPriority2.Value = .Cells(1, 23).Value 'Get the existing value of the priority radio button
                If ABPriority2.Value = 1 Then
                    ABRP6.Value = True
                Else
                    ABRP6.Value = False
                End If
                If ABPriority2.Value = 2 Then
                    ABRP7.Value = True
                Else
                    ABRP7.Value = False
                End If
                If ABPriority2.Value = 3 Then
                    ABRP8.Value = True
                Else
                    ABRP8.Value = False
                End If
                If ABPriority2.Value = 4 Then
                    ABRP9.Value = True
                Else
                    ABRP9.Value = False
                End If
                If ABPriority2.Value = 5 Then
                    ABRP10.Value = True
                Else
                    ABRP10.Value = False
                End If
        ABWho2.Value = .Cells(1, 24).Value
        ABAction2.Value = .Cells(1, 25).Value
        ABWhen2.Value = .Cells(1, 26).Value
        ABStatus2.Value = .Cells(1, 27).Value
        ABFacilitated2.Value = .Cells(1, 28).Value
        ABInformCont2.Value = .Cells(1, 29).Value
        ABAdvised2.Value = .Cells(1, 30).Value
        ABMetexpect2.Value = .Cells(1, 31).Value
        ABChangeOcc2.Value = .Cells(1, 32).Value
        ABAdviserScore2.Value = .Cells(1, 33).Value
            If ABStatus2.Value = "Not Started" Then
                ABAdviserScore2 = 20
            End If
        ABBEScore2.Value = .Cells(1, 34).Value
        ABTotalScore2.Value = .Cells(1, 35).Value
        'Set the visibilty value for radio buttons based on Resolve tick box status true = enabled false = not enabled
        ABWhat2.Enabled = ABResolve2.Value
        ABRecommend2.Enabled = ABResolve2.Value
        ABRP6.Visible = ABResolve2.Value
        ABRP7.Visible = ABResolve2.Value
        ABRP8.Visible = ABResolve2.Value
        ABRP9.Visible = ABResolve2.Value
        ABRP10.Visible = ABResolve2.Value
        ABPriority2.Visible = ABResolve2.Value
        'Set the enabled status per item if recommendation is selected or not, as per resolve tick box
            If ABResolve2.Value = True Then
                ABPriorityLabel2.Caption = "Priority"
                Me.ABPriorityLabel2.ForeColor = RGB(0, 128, 0) 'green
            End If
            If ABResolve2.Value = False Then
                ABPriorityLabel2.Caption = "Not selected for remediation."
                Me.ABPriorityLabel2.ForeColor = RGB(255, 0, 0) 'red
            End If
        ABSuccCrit2.Enabled = ABResolve2.Value
        ABWIR2.Enabled = ABResolve2.Value
        ABWho2.Enabled = ABResolve2.Value
        ABAPA2.Enabled = ABResolve2.Value
        ABAction2.Enabled = ABResolve2.Value
        ABWID2.Enabled = ABResolve2.Value
        ABWhen2.Enabled = ABResolve2.Value
        ABAS2.Enabled = ABResolve2.Value
        ABAdviserScore2.Enabled = ABResolve2.Value
            If ABAdviserScore2.Enabled = False Then
                ABAdviserScore2.Value = 0
            ElseIf ABStatus2.Value = "Not Started" Then
                ABAdviserScore2 = 20
            ElseIf ABStatus2.Value = "Stalled" Then
                ABAdviserScore2.Value = 40
            ElseIf ABStatus2.Value = "In Progress" Then
                ABAdviserScore2.Value = 60
            ElseIf ABStatus2.Value = "Partial" Then
                ABAdviserScore2.Value = 80
            ElseIf ABStatus2.Value = "Completed" Then
                ABAdviserScore2.Value = 100
            End If
        ABASP2.Enabled = ABResolve2.Value
        ABBE2.Enabled = ABResolve2.Value
        'Set the score values to 0 or the actual value if recommend is true or false
        ABBEScore2.Enabled = ABResolve2.Value
            If ABBEScore2.Enabled = False Then
                ABBEScore2.Value = 0
            ElseIf ABBEScore2.Enabled = True Then
                ABBEScore2.Value = .Cells(1, 34).Value
            End If
        ABBESP2.Enabled = ABResolve2.Value
        ABTS2.Enabled = ABResolve2.Value
        'Set the score values to 0 or the actual value if recommend is true or false
        ABTotalScore2.Enabled = ABResolve2.Value
            If ABTotalScore2.Enabled = False Then
                    ABTotalScore2.Value = 0
                ElseIf ABTotalScore2.Enabled = True Then
                    ABTotalScore2 = (ABAdviserScore2.Value / 2) + (ABBEScore2.Value / 2)
            End If
        ABTSP2.Enabled = ABResolve2.Value
        ABStatus2.Enabled = ABResolve2.Value
        ABFacilitated2.Enabled = ABResolve2.Value
        ABInformCont2.Enabled = ABResolve2.Value
        ABAdvised2.Enabled = ABResolve2.Value
        ABMetexpect2.Enabled = ABResolve2.Value
        ABChangeOcc2.Enabled = ABResolve2.Value
        
    End With
        
End Sub
Private Sub Section3(SelectedRow As Range)

   Set LUYBLTable = Worksheets("LUYBL").ListObjects("LUYBL")
    
   With SelectedRow

        ABResponse3.Value = .Cells(1, 36).Value
        ABResolve3.Value = .Cells(1, 37).Value
        ABPriority3.Value = .Cells(1, 38).Value 'Get the existing value of the priority radio button
                If ABPriority3.Value = 1 Then
                    ABRP11.Value = True
                Else
                    ABRP11.Value = False
                End If
                If ABPriority3.Value = 2 Then
                    ABRP12.Value = True
                Else
                    ABRP12.Value = False
                End If
                If ABPriority3.Value = 3 Then
                    ABRP13.Value = True
                Else
                    ABRP13.Value = False
                End If
                If ABPriority3.Value = 4 Then
                    ABRP14.Value = True
                Else
                    ABRP14.Value = False
                End If
                If ABPriority3.Value = 5 Then
                    ABRP15.Value = True
                Else
                    ABRP15.Value = False
                End If
        ABWho3.Value = .Cells(1, 39).Value
        ABAction3.Value = .Cells(1, 40).Value
        ABWhen3.Value = .Cells(1, 41).Value
        ABStatus3.Value = .Cells(1, 42).Value
        ABFacilitated3.Value = .Cells(1, 43).Value
        ABInformCont3.Value = .Cells(1, 44).Value
        ABAdvised3.Value = .Cells(1, 45).Value
        ABMetexpect3.Value = .Cells(1, 46).Value
        ABChangeOcc3.Value = .Cells(1, 47).Value
        ABAdviserScore3.Value = .Cells(1, 48).Value
            If ABStatus3.Value = "Not Started" Then
                ABAdviserScore3 = 20
            End If
        ABBEScore3.Value = .Cells(1, 49).Value
        ABTotalScore3.Value = .Cells(1, 50).Value
        'Set the visibilty value for radio buttons based on Resolve tick box status true = enabled false = not enabled
        ABWhat3.Enabled = ABResolve3.Value
        ABRecommend3.Enabled = ABResolve3.Value
        ABRP11.Visible = ABResolve3.Value
        ABRP12.Visible = ABResolve3.Value
        ABRP13.Visible = ABResolve3.Value
        ABRP14.Visible = ABResolve3.Value
        ABRP15.Visible = ABResolve3.Value
        ABPriority3.Visible = ABResolve3.Value
        'Set the enabled status per item if recommendation is selected or not, as per resolve tick box
            If ABResolve3.Value = True Then
                ABPriorityLabel3.Caption = "Priority"
                Me.ABPriorityLabel3.ForeColor = RGB(0, 128, 0) 'green
            End If
            If ABResolve3.Value = False Then
                ABPriorityLabel3.Caption = "Not selected for remediation."
                Me.ABPriorityLabel3.ForeColor = RGB(255, 0, 0) 'red
            End If
        ABSuccCrit3.Enabled = ABResolve3.Value
        ABWIR3.Enabled = ABResolve3.Value
        ABWho3.Enabled = ABResolve3.Value
        ABAPA3.Enabled = ABResolve3.Value
        ABAction3.Enabled = ABResolve3.Value
        ABWID3.Enabled = ABResolve3.Value
        ABWhen3.Enabled = ABResolve3.Value
        ABAS3.Enabled = ABResolve3.Value
        ABAdviserScore3.Enabled = ABResolve3.Value
            If ABAdviserScore3.Enabled = False Then
                ABAdviserScore3.Value = 0
            ElseIf ABStatus3.Value = "Not Started" Then
                ABAdviserScore3 = 20
            ElseIf ABStatus3.Value = "Stalled" Then
                ABAdviserScore3.Value = 40
            ElseIf ABStatus3.Value = "In Progress" Then
                ABAdviserScore3.Value = 60
            ElseIf ABStatus3.Value = "Partial" Then
                ABAdviserScore3.Value = 80
            ElseIf ABStatus3.Value = "Completed" Then
                ABAdviserScore3.Value = 100
            End If
        ABASP3.Enabled = ABResolve3.Value
        ABBE3.Enabled = ABResolve3.Value
        'Set the score values to 0 or the actual value if recommend is true or false
        ABBEScore3.Enabled = ABResolve3.Value
            If ABBEScore3.Enabled = False Then
                ABBEScore3.Value = 0
            ElseIf ABBEScore3.Enabled = True Then
                ABBEScore3.Value = .Cells(1, 49).Value
            End If
        ABBESP3.Enabled = ABResolve3.Value
        ABTS3.Enabled = ABResolve3.Value
        'Set the score values to 0 or the actual value if recommend is true or false
        ABTotalScore3.Enabled = ABResolve3.Value
            If ABTotalScore3.Enabled = False Then
                    ABTotalScore3.Value = 0
                ElseIf ABTotalScore3.Enabled = True Then
                    ABTotalScore3 = (ABAdviserScore3.Value / 2) + (ABBEScore3.Value / 2)
            End If
        ABTSP3.Enabled = ABResolve3.Value
        ABStatus3.Enabled = ABResolve3.Value
        ABFacilitated3.Enabled = ABResolve3.Value
        ABInformCont3.Enabled = ABResolve3.Value
        ABAdvised3.Enabled = ABResolve3.Value
        ABMetexpect3.Enabled = ABResolve3.Value
        ABChangeOcc3.Enabled = ABResolve3.Value
        
        End With
        
