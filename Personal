End If

'       Remainder of the values go here


' Control the visibility of the tenure based question pages on loading of the form for the first time
'Tenure A and B
    If Tenure.Value = "A" Then
        Me.TenCont.pages(0).Visible = True
        Me.TenCont.pages(1).Visible = False
        Me.TenCont.pages(2).Visible = False
        Personal.TenCont.Value = 0
        Personal.MultiPage3.Value = 0
        ElseIf Tenure.Value = "B" Then
        Me.TenCont.pages(0).Visible = True
        Me.TenCont.pages(1).Visible = False
        Me.TenCont.pages(2).Visible = False
        Personal.TenCont.Value = 0
        Personal.MultiPage3.Value = 0
    End If
'Tenure C and D
    If Tenure.Value = "C" Then
        Me.TenCont.pages(0).Visible = False
        Me.TenCont.pages(1).Visible = True
        Me.TenCont.pages(2).Visible = False
        Personal.TenCont.Value = 1
        Personal.MultiPage2.Value = 0
        ElseIf Tenure.Value = "D" Then
        Me.TenCont.pages(0).Visible = False
        Me.TenCont.pages(1).Visible = True
        Me.TenCont.pages(2).Visible = False
        Personal.TenCont.Value = 1
        Personal.MultiPage2.Value = 0
    End If
'Tenure E and F and R
    If Tenure.Value = "E" Then
        Me.TenCont.pages(0).Visible = False
        Me.TenCont.pages(1).Visible = False
        Me.TenCont.pages(2).Visible = True
        Personal.TenCont.Value = 2
        Personal.MultiPage1.Value = 0
        ElseIf Tenure.Value = "F" Then
        Me.TenCont.pages(0).Visible = False
        Me.TenCont.pages(1).Visible = False
        Me.TenCont.pages(2).Visible = True
        Personal.TenCont.Value = 2
        Personal.MultiPage1.Value = 0
        ElseIf Tenure.Value = "R" Then
        Me.TenCont.pages(0).Visible = False
        Me.TenCont.pages(1).Visible = False
        Me.TenCont.pages(2).Visible = True
        Personal.TenCont.Value = 2
        Personal.MultiPage1.Value = 0
    End If

End Sub
Private Sub ABResolve1_Click()
'Hide or show the recommendation text boxs and priorities if a recommendation must be made.
' Section 1
        ABWhat1.Enabled = ABResolve1.Value
        ABRecommend1.Enabled = ABResolve1.Value
        ABRP1.Visible = ABResolve1.Value
        ABRP2.Visible = ABResolve1.Value
        ABRP3.Visible = ABResolve1.Value
        ABRp4.Visible = ABResolve1.Value
        ABRP5.Visible = ABResolve1.Value
        ABPriority1.Visible = ABResolve1.Value
'       Set the status per recommendation if selected or not to resolve1
        If ABResolve1.Value = True Then
            ABPriorityLabel1.Caption = "Priority"
            Me.ABPriorityLabel1.ForeColor = RGB(0, 128, 0) 'green
        End If
        If ABResolve1.Value = False Then
            Me.ABPriorityLabel1.ForeColor = RGB(255, 0, 0) 'red
            ABPriorityLabel1.Caption = "Not selected for remediation."
        End If
       ABSuccCrit1.Enabled = ABResolve1.Value
        ABWIR1.Enabled = ABResolve1.Value
        ABWho1.Enabled = ABResolve1.Value
        ABAPA1.Enabled = ABResolve1.Value
        ABAction1.Enabled = ABResolve1.Value
        ABWID1.Enabled = ABResolve1.Value
        ABWhen1.Enabled = ABResolve1.Value
        ABAS1.Enabled = ABResolve1.Value
        ABAdviserScore1.Enabled = ABResolve1.Value
            If ABAdviserScore1.Enabled = False Then
                ABAdviserScore1.Value = 0
            ElseIf ABStatus1.Value = "Not Started" Then
                ABAdviserScore1 = 20
            ElseIf ABStatus1.Value = "Stalled" Then
                ABAdviserScore1.Value = 40
            ElseIf ABStatus1.Value = "In Progress" Then
                ABAdviserScore1.Value = 60
            ElseIf ABStatus1.Value = "Partial" Then
                ABAdviserScore1.Value = 80
            ElseIf ABStatus1.Value = "Completed" Then
                ABAdviserScore1.Value = 100
            End If
        ABASP1.Enabled = ABResolve1.Value
        ABBE1.Enabled = ABResolve1.Value
        ABBEScore1.Enabled = ABResolve1.Value
            If ABBEScore1.Enabled = False Then
                ABBEScore1.Value = 0
            ElseIf ABBEScore1.Enabled = True Then
                If ABFacilitated1.Value = True Then
                    ABBEScore1.Value = ABBEScore1.Value + 20
                End If
                If ABInformCont1.Value = True Then
                    ABBEScore1.Value = ABBEScore1.Value + 20
                End If
                If ABAdvised1.Value = True Then
                    ABBEScore1.Value = ABBEScore1.Value + 20
                End If
                If ABMetexpect1.Value = True Then
                    ABBEScore1.Value = ABBEScore1.Value + 20
                End If
                If ABChangeOcc1.Value = True Then
                    ABBEScore1.Value = ABBEScore1.Value + 20
                End If
            End If
        ABBESP1.Enabled = ABResolve1.Value
        ABTS1.Enabled = ABResolve1.Value
        ABTotalScore1.Enabled = ABResolve1.Value
            If ABTotalScore1.Enabled = False Then
                    ABTotalScore1.Value = 0
                ElseIf ABTotalScore1.Enabled = True Then
                    ABTotalScore1 = (ABAdviserScore1.Value / 2) + (ABBEScore1.Value / 2)
            End If
        ABTSP1.Enabled = ABResolve1.Value
        ABStatus1.Enabled = ABResolve1.Value
        ABFacilitated1.Enabled = ABResolve1.Value
        ABInformCont1.Enabled = ABResolve1.Value
        ABAdvised1.Enabled = ABResolve1.Value
        ABMetexpect1.Enabled = ABResolve1.Value
        ABChangeOcc1.Enabled = ABResolve1.Value
End Sub
Private Sub ABResolve2_Click()
'Hide or show the recommendation text boxs and priorities if a recommendation must be made.
'Section 2
        ABWhat2.Enabled = ABResolve2.Value
        ABRecommend2.Enabled = ABResolve2.Value
        ABRP6.Visible = ABResolve2.Value
        ABRP7.Visible = ABResolve2.Value
        ABRP8.Visible = ABResolve2.Value
        ABRP9.Visible = ABResolve2.Value
        ABRP10.Visible = ABResolve2.Value
        ABPriority2.Visible = ABResolve2.Value
'       Set the status per recommendation if selected or not to resolve1
        If ABResolve2.Value = True Then
            ABPriorityLabel2.Caption = "Priority"
            Me.ABPriorityLabel2.ForeColor = RGB(0, 128, 0) 'green
        End If
        If ABResolve2.Value = False Then
            ABPriorityLabel2.Caption = "Not selected for remediation."
            Me.ABPriorityLabel2.ForeColor = RGB(255, 0, 0) 'red
        End If
        ABSuccCrit2.Enabled = ABResolve2.Value
        ABWIR2.Enabled = ABResolve2.Value
        ABWho2.Enabled = ABResolve2.Value
        ABAPA2.Enabled = ABResolve2.Value
        ABAction2.Enabled = ABResolve2.Value
        ABWID2.Enabled = ABResolve2.Value
        ABWhen2.Enabled = ABResolve2.Value
        ABAS2.Enabled = ABResolve2.Value
        ABAdviserScore2.Enabled = ABResolve2.Value
            If ABAdviserScore2.Enabled = False Then
                ABAdviserScore2.Value = 0
            ElseIf ABStatus2.Value = "Not Started" Then
                ABAdviserScore2 = 20
            ElseIf ABStatus2.Value = "Stalled" Then
                ABAdviserScore2.Value = 40
            ElseIf ABStatus2.Value = "In Progress" Then
                ABAdviserScore2.Value = 60
            ElseIf ABStatus2.Value = "Partial" Then
                ABAdviserScore2.Value = 80
            ElseIf ABStatus2.Value = "Completed" Then
                ABAdviserScore2.Value = 100
            End If
        ABASP2.Enabled = ABResolve2.Value
        ABBE2.Enabled = ABResolve2.Value
        ABBEScore2.Enabled = ABResolve2.Value
            If ABBEScore2.Enabled = False Then
                ABBEScore2.Value = 0
            ElseIf ABBEScore2.Enabled = True Then
                If ABFacilitated2.Value = True Then
                    ABBEScore2.Value = ABBEScore2.Value + 20
                End If
                If ABInformCont2.Value = True Then
                    ABBEScore2.Value = ABBEScore2.Value + 20
                End If
                If ABAdvised2.Value = True Then
                    ABBEScore2.Value = ABBEScore2.Value + 20
                End If
                If ABMetexpect2.Value = True Then
                    ABBEScore2.Value = ABBEScore2.Value + 20
                End If
                If ABChangeOcc2.Value = True Then
                    ABBEScore2.Value = ABBEScore2.Value + 20
                End If
            End If
        ABBESP2.Enabled = ABResolve2.Value
        ABTS2.Enabled = ABResolve2.Value
        ABTotalScore2.Enabled = ABResolve2.Value
            If ABTotalScore2.Enabled = False Then
                    ABTotalScore2.Value = 0
                ElseIf ABTotalScore2.Enabled = True Then
                    ABTotalScore2 = (ABAdviserScore2.Value / 2) + (ABBEScore2.Value / 2)
            End If
        ABTSP2.Enabled = ABResolve2.Value
        ABStatus2.Enabled = ABResolve2.Value
        ABFacilitated2.Enabled = ABResolve2.Value
        ABInformCont2.Enabled = ABResolve2.Value
        ABAdvised2.Enabled = ABResolve2.Value
        ABMetexpect2.Enabled = ABResolve2.Value
        ABChangeOcc2.Enabled = ABResolve2.Value
End Sub
