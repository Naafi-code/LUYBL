    ElseIf ABBEScore24.Value <> 0 Then
        ABBEScore24 = ABBEScore24.Value - 20
    End If
    ABTotalScore24 = (ABAdviserScore24.Value / 2) + (ABBEScore24.Value / 2)
End Sub
Private Sub ABMetexpect24_Click()
    'Set the value of the BE score when the check box is ticked or unticked.
'Section 24
    If ABMetexpect24.Value = True Then
        ABBEScore24 = ABBEScore24.Value + 20
    ElseIf ABBEScore24.Value <> 0 Then
        ABBEScore24 = ABBEScore24.Value - 20
    End If
