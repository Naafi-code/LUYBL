Private Sub ABStatus1_Change()
    'Set the vlaue of the Adviser Score according to the status selected in the Status combobox
    'Section 1
    If ABStatus1.Value = "Not Started" Then
        ABAdviserScore1.Value = 20
    ElseIf ABStatus1.Value = "Stalled" Then
        ABAdviserScore1.Value = 40
    ElseIf ABStatus1.Value = "In Progress" Then
        ABAdviserScore1.Value = 60
    ElseIf ABStatus1.Value = "Partial" Then
        ABAdviserScore1.Value = 80
    ElseIf ABStatus1.Value = "Completed" Then
        ABAdviserScore1.Value = 100
    End If
    If ABBEScore1 = "" Then 'Avoid division by zero on deleted record
        ABBEScore1 = 1
    End If
    ABTotalScore1 = (ABAdviserScore1.Value / 2) + (ABBEScore1.Value / 2)
End Sub
