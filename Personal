Private Sub Tenure_Click()
'Control the visibility of the tenure based question pages on changing of the tenure combobox
'Tenure A and B
    If Tenure.Value = "A" Then
        Me.TenCont.pages(0).Visible = True
        Me.TenCont.pages(1).Visible = False
        Me.TenCont.pages(2).Visible = False
        Personal.TenCont.Value = 0
        Personal.MultiPage3.Value = 0
        ElseIf Tenure.Value = "B" Then
        Me.TenCont.pages(0).Visible = True
        Me.TenCont.pages(1).Visible = False
        Me.TenCont.pages(2).Visible = False
        Personal.TenCont.Value = 0
        Personal.MultiPage3.Value = 0
    End If
'Tenure C and D
    If Tenure.Value = "C" Then
        Me.TenCont.pages(0).Visible = False
        Me.TenCont.pages(1).Visible = True
        Me.TenCont.pages(2).Visible = False
        Personal.TenCont.Value = 1
        Personal.MultiPage2.Value = 0
        ElseIf Tenure.Value = "D" Then
        Me.TenCont.pages(0).Visible = False
        Me.TenCont.pages(1).Visible = True
        Me.TenCont.pages(2).Visible = False
        Personal.TenCont.Value = 1
        Personal.MultiPage2.Value = 0
    End If
'Tenure E and F and R
    If Tenure.Value = "E" Then
        Me.TenCont.pages(0).Visible = False
        Me.TenCont.pages(1).Visible = False
        Me.TenCont.pages(2).Visible = True
        Personal.TenCont.Value = 2
        Personal.MultiPage1.Value = 0
        ElseIf Tenure.Value = "F" Then
        Me.TenCont.pages(0).Visible = False
        Me.TenCont.pages(1).Visible = False
        Me.TenCont.pages(2).Visible = True
        Personal.TenCont.Value = 2
        Personal.MultiPage1.Value = 0
        ElseIf Tenure.Value = "R" Then
        Me.TenCont.pages(0).Visible = False
        Me.TenCont.pages(1).Visible = False
        Me.TenCont.pages(2).Visible = True
        Personal.TenCont.Value = 2
        Personal.MultiPage1.Value = 0
    End If

End Sub
Private Sub ChangeRecord_Click()
'Control the visibility of the tenure based question pages on changing of the tenure combobox
'Tenure A and B
    If Tenure.Value = "A" Then
        Me.TenCont.pages(0).Visible = True
        Me.TenCont.pages(1).Visible = False
        Me.TenCont.pages(2).Visible = False
