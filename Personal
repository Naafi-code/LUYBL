                Me.ABPriorityLabel14.ForeColor = RGB(255, 0, 0) 'red
            End If
        ABSuccCrit14.Enabled = ABResolve14.Value
        ABWIR14.Enabled = ABResolve14.Value
        ABWho14.Enabled = ABResolve14.Value
        ABAPA14.Enabled = ABResolve14.Value
        ABAction14.Enabled = ABResolve14.Value
        ABWID14.Enabled = ABResolve14.Value
        ABWhen14.Enabled = ABResolve14.Value
        ABAS14.Enabled = ABResolve14.Value
        ABAdviserScore14.Enabled = ABResolve14.Value
            If ABAdviserScore14.Enabled = False Then
                ABAdviserScore14 = 0
            ElseIf ABStatus14.Value = "Not Started" Then
                ABAdviserScore14.Value = 20
            ElseIf ABStatus14.Value = "Stalled" Then
               ABAdviserScore14.Value = 40
            ElseIf ABStatus14.Value = "In Progress" Then
                ABAdviserScore14.Value = 60
            ElseIf ABStatus14.Value = "Partial" Then
                ABAdviserScore14.Value = 80
            ElseIf ABStatus14.Value = "Completed" Then
                ABAdviserScore14.Value = 100
            End If
        ABASP14.Enabled = ABResolve14.Value
        ABBE14.Enabled = ABResolve14.Value
        'Set the score values to 0 or the actual value if recommend is true or false
        ABBEScore14.Enabled = ABResolve14.Value
            If ABBEScore14.Enabled = False Then
                ABBEScore14.Value = 0
            ElseIf ABBEScore14.Enabled = True Then
                ABBEScore14.Value = .Cells(1, 214).Value
            End If
        ABBESP14.Enabled = ABResolve14.Value
        ABTS14.Enabled = ABResolve14.Value
        'Set the score values to 0 or the actual value if recommend is true or false
        ABTotalScore14.Enabled = ABResolve14.Value
            If ABTotalScore14.Enabled = False Then
                    ABTotalScore14.Value = 0
                ElseIf ABTotalScore14.Enabled = True Then
                    ABTotalScore14 = (ABAdviserScore14.Value / 2) + (ABBEScore14.Value / 2)
            End If
        ABTSP14.Enabled = ABResolve14.Value
        ABStatus14.Enabled = ABResolve14.Value
        ABFacilitated14.Enabled = ABResolve14.Value
        ABInformCont14.Enabled = ABResolve14.Value
        ABAdvised14.Enabled = ABResolve14.Value
        ABMetexpect14.Enabled = ABResolve14.Value
        ABChangeOcc14.Enabled = ABResolve14.Value
                
        End With
        
End Sub
