Private Sub ABResolve11_Click()
'Hide or show the recommendation text boxs and priorities if a recommendation must be made.
'Section 11
        ABWhat11.Enabled = ABResolve11.Value
        ABRecommend11.Enabled = ABResolve11.Value
        ABRP51.Visible = ABResolve11.Value
        ABRP52.Visible = ABResolve11.Value
        ABRP53.Visible = ABResolve11.Value
        ABRP54.Visible = ABResolve11.Value
        ABRP55.Visible = ABResolve11.Value
        ABPriority11.Visible = ABResolve11.Value
'       Set the status per recommendation if selected or not to resolve1
        If ABResolve11.Value = True Then
            ABPriorityLabel11.Caption = "Priority"
            Me.ABPriorityLabel11.ForeColor = RGB(0, 128, 0) 'green
        End If
        If ABResolve11.Value = False Then
            ABPriorityLabel11.Caption = "Not selected for remediation."
            Me.ABPriorityLabel11.ForeColor = RGB(255, 0, 0) 'red
        End If
        ABSuccCrit11.Enabled = ABResolve11.Value
        ABWIR11.Enabled = ABResolve11.Value
        ABWho11.Enabled = ABResolve11.Value
        ABAPA11.Enabled = ABResolve11.Value
        ABAction11.Enabled = ABResolve11.Value
        ABWID11.Enabled = ABResolve11.Value
        ABWhen11.Enabled = ABResolve11.Value
        ABAS11.Enabled = ABResolve11.Value
        ABAdviserScore11.Enabled = ABResolve11.Value
            If ABAdviserScore11.Enabled = False Then
                ABAdviserScore11.Value = 0
            ElseIf ABStatus11.Value = "Not Started" Then
                ABAdviserScore6 = 20
            ElseIf ABStatus11.Value = "Stalled" Then
                ABAdviserScore11.Value = 40
            ElseIf ABStatus11.Value = "In Progress" Then
                ABAdviserScore11.Value = 60
            ElseIf ABStatus11.Value = "Partial" Then
                ABAdviserScore11.Value = 80
            ElseIf ABStatus11.Value = "Completed" Then
                ABAdviserScore11.Value = 100
            End If
        ABASP11.Enabled = ABResolve11.Value
        ABBE11.Enabled = ABResolve11.Value
        ABBEScore11.Enabled = ABResolve11.Value
            If ABBEScore11.Enabled = False Then
                ABBEScore11.Value = 0
            ElseIf ABBEScore11.Enabled = True Then
                If ABFacilitated11.Value = True Then
                    ABBEScore11.Value = ABBEScore11.Value + 20
                End If
                If ABInformCont11.Value = True Then
                    ABBEScore11.Value = ABBEScore11.Value + 20
                End If
                If ABAdvised11.Value = True Then
                    ABBEScore11.Value = ABBEScore11.Value + 20
                End If
                If ABMetexpect11.Value = True Then
                    ABBEScore11.Value = ABBEScore11.Value + 20
                End If
                If ABChangeOcc11.Value = True Then
                    ABBEScore11.Value = ABBEScore11.Value + 20
                End If
            End If
        ABBESP11.Enabled = ABResolve11.Value
        ABTS11.Enabled = ABResolve11.Value
        ABTotalScore11.Enabled = ABResolve11.Value
            If ABTotalScore11.Enabled = False Then
                    ABTotalScore11.Value = 0
                ElseIf ABTotalScore11.Enabled = True Then
                    ABTotalScore11 = (ABAdviserScore11.Value / 2) + (ABBEScore11.Value / 2)
            End If
        ABTSP11.Enabled = ABResolve11.Value
        ABStatus11.Enabled = ABResolve11.Value
        ABFacilitated11.Enabled = ABResolve11.Value
        ABInformCont11.Enabled = ABResolve11.Value
        ABAdvised11.Enabled = ABResolve11.Value
        ABMetexpect11.Enabled = ABResolve11.Value
        ABChangeOcc11.Enabled = ABResolve11.Value
End Sub
