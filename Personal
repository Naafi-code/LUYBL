Private Sub ABFacilitated1_Click()
    'Set the value of the BE score when the check box is ticked or unticked.
'Section 1
    If ABFacilitated1.Value = True Then
        ABBEScore1 = ABBEScore1.Value + 20
    ElseIf ABBEScore1.Value <> 0 Then
        ABBEScore1 = ABBEScore1.Value - 20
    End If
    ABTotalScore1 = (ABAdviserScore1.Value / 2) + (ABBEScore1.Value / 2)
End Sub
