        ABBESP3.Enabled = ABResolve3.Value
        ABTS3.Enabled = ABResolve3.Value
        ABTotalScore3.Enabled = ABResolve3.Value
            If ABTotalScore3.Enabled = False Then
                    ABTotalScore3.Value = 0
                ElseIf ABTotalScore3.Enabled = True Then
                    ABTotalScore3 = (ABAdviserScore3.Value / 2) + (ABBEScore3.Value / 2)
            End If
        ABTSP3.Enabled = ABResolve3.Value
        ABStatus3.Enabled = ABResolve3.Value
        ABFacilitated3.Enabled = ABResolve3.Value
        ABInformCont3.Enabled = ABResolve3.Value
        ABAdvised3.Enabled = ABResolve3.Value
        ABMetexpect3.Enabled = ABResolve3.Value
        ABChangeOcc3.Enabled = ABResolve3.Value
End Sub
Private Sub ABResolve4_Click()
'Hide or show the recommendation text boxs and priorities if a recommendation must be made.
'Section 4
        ABWhat4.Enabled = ABResolve4.Value
        ABRecommend4.Enabled = ABResolve4.Value
        ABRP16.Visible = ABResolve4.Value
        ABRP17.Visible = ABResolve4.Value
        ABRP18.Visible = ABResolve4.Value
        ABRP19.Visible = ABResolve4.Value
        ABRP20.Visible = ABResolve4.Value
        ABPriority4.Visible = ABResolve4.Value
'       Set the status per recommendation if selected or not to resolve1
        If ABResolve4.Value = True Then
            ABPriorityLabel4.Caption = "Priority"
            Me.ABPriorityLabel4.ForeColor = RGB(0, 128, 0) 'green
        End If
        If ABResolve4.Value = False Then
            ABPriorityLabel4.Caption = "Not selected for remediation."
            Me.ABPriorityLabel4.ForeColor = RGB(255, 0, 0) 'red
        End If
        ABSuccCrit4.Enabled = ABResolve4.Value
        ABWIR4.Enabled = ABResolve4.Value
        ABWho4.Enabled = ABResolve4.Value
        ABAPA4.Enabled = ABResolve4.Value
        ABAction4.Enabled = ABResolve4.Value
        ABWID4.Enabled = ABResolve4.Value
        ABWhen4.Enabled = ABResolve4.Value
        ABAS4.Enabled = ABResolve4.Value
        ABAdviserScore4.Enabled = ABResolve4.Value
            If ABAdviserScore4.Enabled = False Then
                ABAdviserScore4.Value = 0
            ElseIf ABStatus4.Value = "Not Started" Then
                ABAdviserScore4 = 20
            ElseIf ABStatus4.Value = "Stalled" Then
                ABAdviserScore4.Value = 40
            ElseIf ABStatus4.Value = "In Progress" Then
                ABAdviserScore4.Value = 60
            ElseIf ABStatus4.Value = "Partial" Then
                ABAdviserScore4.Value = 80
            ElseIf ABStatus4.Value = "Completed" Then
                ABAdviserScore4.Value = 100
            End If
        ABASP4.Enabled = ABResolve4.Value
        ABBE4.Enabled = ABResolve4.Value
        ABBEScore4.Enabled = ABResolve4.Value
            If ABBEScore4.Enabled = False Then
                ABBEScore4.Value = 0
            ElseIf ABBEScore4.Enabled = True Then
                If ABFacilitated4.Value = True Then
                    ABBEScore4.Value = ABBEScore4.Value + 20
                End If
                If ABInformCont4.Value = True Then
                    ABBEScore4.Value = ABBEScore4.Value + 20
                End If
                If ABAdvised4.Value = True Then
                    ABBEScore4.Value = ABBEScore4.Value + 20
                End If
                If ABMetexpect4.Value = True Then
                    ABBEScore4.Value = ABBEScore4.Value + 20
                End If
                If ABChangeOcc4.Value = True Then
                    ABBEScore4.Value = ABBEScore4.Value + 20
                End If
            End If
        ABBESP4.Enabled = ABResolve4.Value
        ABTS4.Enabled = ABResolve4.Value
        ABTotalScore4.Enabled = ABResolve4.Value
            If ABTotalScore4.Enabled = False Then
                    ABTotalScore4.Value = 0
                ElseIf ABTotalScore4.Enabled = True Then
                    ABTotalScore4 = (ABAdviserScore4.Value / 2) + (ABBEScore4.Value / 2)
            End If
        ABTSP4.Enabled = ABResolve4.Value
        ABStatus4.Enabled = ABResolve4.Value
        ABFacilitated4.Enabled = ABResolve4.Value
        ABInformCont4.Enabled = ABResolve4.Value
        ABAdvised4.Enabled = ABResolve4.Value
        ABMetexpect4.Enabled = ABResolve4.Value
        ABChangeOcc4.Enabled = ABResolve4.Value
End Sub
