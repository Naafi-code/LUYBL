Option Explicit
Private LUYBLTable As ListObject
Private QuestionTable As ListObject
Private PRINTTable As ListObject
Private CurrentRow As Long
Private CurrentRow1 As Long

Public Sub UserForm_Initialize()

'Starts the Mouse Scroll Capability
    Me.StartUpPosition = 0
    Me.Left = Application.Left + Application.Width / 2 - Me.Width / 2
    Me.Top = Application.Top + Application.Height / 2 - Me.Height / 2

    EnableMouseScroll Me
    
'Starts the FORM data insert
    Set LUYBLTable = Worksheets("LUYBL").ListObjects("LUYBL")

          
    'Initialise for empty table
    ChangeRecord.Min = 0
    ChangeRecord.Max = 0

    CurrentRow = LUYBLTable.ListRows.Count
    
    If CurrentRow > 0 Then
    
        ChangeRecord.Min = 1
        ChangeRecord.Max = LUYBLTable.ListRows.Count
        
        'Load last record into form
         Section1 LUYBLTable.ListRows(LUYBLTable.ListRows.Count).Range
         Section2 LUYBLTable.ListRows(LUYBLTable.ListRows.Count).Range
         Section3 LUYBLTable.ListRows(LUYBLTable.ListRows.Count).Range
         Section4 LUYBLTable.ListRows(LUYBLTable.ListRows.Count).Range
         Section5 LUYBLTable.ListRows(LUYBLTable.ListRows.Count).Range
         Section6 LUYBLTable.ListRows(LUYBLTable.ListRows.Count).Range
         Section7 LUYBLTable.ListRows(LUYBLTable.ListRows.Count).Range
         Section8 LUYBLTable.ListRows(LUYBLTable.ListRows.Count).Range
         Section9 LUYBLTable.ListRows(LUYBLTable.ListRows.Count).Range
         Section10 LUYBLTable.ListRows(LUYBLTable.ListRows.Count).Range
         Section11 LUYBLTable.ListRows(LUYBLTable.ListRows.Count).Range
         Section12 LUYBLTable.ListRows(LUYBLTable.ListRows.Count).Range
         Section13 LUYBLTable.ListRows(LUYBLTable.ListRows.Count).Range
         Section14 LUYBLTable.ListRows(LUYBLTable.ListRows.Count).Range
         Section15 LUYBLTable.ListRows(LUYBLTable.ListRows.Count).Range
         Section16 LUYBLTable.ListRows(LUYBLTable.ListRows.Count).Range
         Section17 LUYBLTable.ListRows(LUYBLTable.ListRows.Count).Range
         Section18 LUYBLTable.ListRows(LUYBLTable.ListRows.Count).Range
         Section19 LUYBLTable.ListRows(LUYBLTable.ListRows.Count).Range
         Section20 LUYBLTable.ListRows(LUYBLTable.ListRows.Count).Range
         Section21 LUYBLTable.ListRows(LUYBLTable.ListRows.Count).Range
         Section22 LUYBLTable.ListRows(LUYBLTable.ListRows.Count).Range
         Section23 LUYBLTable.ListRows(LUYBLTable.ListRows.Count).Range
         Section24 LUYBLTable.ListRows(LUYBLTable.ListRows.Count).Range
         Section25 LUYBLTable.ListRows(LUYBLTable.ListRows.Count).Range
         Section26 LUYBLTable.ListRows(LUYBLTable.ListRows.Count).Range

         
        UpdatePositionCaption
        
    Else
    
        RecordPosition.Caption = "Row 0 of 0 Rows."
        
    End If
    
    Set QuestionTable = Worksheets("Question").ListObjects("Question")
    
    CurrentRow1 = 1
        
    If CurrentRow1 > 0 Then
    
        'Load first record into form
         PopulateForm1 QuestionTable.ListRows(CurrentRow1).Range
        'Load recommendations to recommendations tab
         PopulateForm2 QuestionTable.ListRows(CurrentRow1).Range
        
    End If
    
'Set the drop down values for Tenure combo box
    Tenure.AddItem "A"
    Tenure.AddItem "B"
    Tenure.AddItem "C"
    Tenure.AddItem "D"
    Tenure.AddItem "E"
    Tenure.AddItem "F"
    Tenure.AddItem "R"
    'Set the drop down values for Status combo box
'Section 1
    ABStatus1.AddItem "Not Started"
    ABStatus1.AddItem "Stalled"
    ABStatus1.AddItem "In Progress"
    ABStatus1.AddItem "Partial"
    ABStatus1.AddItem "Completed"
'Section 2
    ABStatus2.AddItem "Not Started"
    ABStatus2.AddItem "Stalled"
    ABStatus2.AddItem "In Progress"
    ABStatus2.AddItem "Partial"
    ABStatus2.AddItem "Completed"
'Section 3
    ABStatus3.AddItem "Not Started"
    ABStatus3.AddItem "Stalled"
    ABStatus3.AddItem "In Progress"
    ABStatus3.AddItem "Partial"
    ABStatus3.AddItem "Completed"
'Section 4
    ABStatus4.AddItem "Not Started"
    ABStatus4.AddItem "Stalled"
    ABStatus4.AddItem "In Progress"
    ABStatus4.AddItem "Partial"
    ABStatus4.AddItem "Completed"
'Section 5
    ABStatus5.AddItem "Not Started"
    ABStatus5.AddItem "Stalled"
    ABStatus5.AddItem "In Progress"
    ABStatus5.AddItem "Partial"
    ABStatus5.AddItem "Completed"
'Section 6
    ABStatus6.AddItem "Not Started"
    ABStatus6.AddItem "Stalled"
    ABStatus6.AddItem "In Progress"
    ABStatus6.AddItem "Partial"
    ABStatus6.AddItem "Completed"
'Section 7
    ABStatus7.AddItem "Not Started"
    ABStatus7.AddItem "Stalled"
    ABStatus7.AddItem "In Progress"
    ABStatus7.AddItem "Partial"
    ABStatus7.AddItem "Completed"
'Section 8
    ABStatus8.AddItem "Not Started"
    ABStatus8.AddItem "Stalled"
    ABStatus8.AddItem "In Progress"
    ABStatus8.AddItem "Partial"
    ABStatus8.AddItem "Completed"
'Section 9
    ABStatus9.AddItem "Not Started"
