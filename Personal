Private Sub PopulateForm2(CurrentRow1 As Range)

    'Populates the Recommendations
    Set QuestionTable = Worksheets("Question").ListObjects("Question")
    
    With CurrentRow1
'Section 1
        ABRecommend1.Value = .Cells(1, 4).Value
        ABWhat1.Value = .Cells(1, 5).Value
'Section 2
        ABRecommend2.Value = .Cells(2, 4).Value
        ABWhat2.Value = .Cells(2, 5).Value
'Section 3
        ABRecommend3.Value = .Cells(3, 4).Value & " ...and... " & .Cells(4, 4).Value
        ABWhat3.Value = .Cells(3, 5).Value & " ...and... " & .Cells(4, 5).Value
'Section 4
        ABRecommend4.Value = .Cells(7, 4).Value
        ABWhat4.Value = .Cells(7, 5).Value
'Section 5
        ABRecommend5.Value = .Cells(9, 4).Value
        ABWhat5.Value = .Cells(9, 5).Value
'Section 6
        ABRecommend6.Value = .Cells(12, 4).Value
        ABWhat5.Value = .Cells(12, 5).Value
'Section 7
        ABRecommend7.Value = .Cells(17, 4).Value
        ABWhat7.Value = .Cells(17, 5).Value
'Section 8
        ABRecommend8.Value = .Cells(18, 4).Value
        ABWhat8.Value = .Cells(18, 5).Value
'Section 9
        ABRecommend9.Value = .Cells(27, 4).Value & " ...and... " & .Cells(28, 4).Value
        ABWhat9.Value = .Cells(27, 5).Value & " ...and... " & .Cells(28, 5).Value
'Section 10
        ABRecommend10.Value = .Cells(29, 4).Value & " ...and... " & .Cells(30, 4).Value
        ABWhat10.Value = .Cells(29, 5).Value
'Section 11
        ABRecommend11.Value = .Cells(31, 4).Value
        ABWhat11.Value = .Cells(31, 5).Value
'Section 12
        ABRecommend12.Value = .Cells(32, 4).Value
        ABWhat12.Value = .Cells(32, 5).Value
'Section 13
        ABRecommend13.Value = .Cells(33, 4).Value
        ABWhat13.Value = .Cells(33, 5).Value
'Section 14
        ABRecommend14.Value = .Cells(34, 4).Value & " ...AND... " & .Cells(35, 4).Value
        ABWhat14.Value = .Cells(34, 5).Value & " ...AND... " & .Cells(35, 5).Value
'Section 15
        ABRecommend15.Value = .Cells(36, 4).Value
        ABWhat15.Value = .Cells(36, 5).Value
'Section 16
        ABRecommend16.Value = .Cells(37, 4).Value
        ABWhat16.Value = .Cells(37, 5).Value
'Section 17
        ABRecommend17.Value = .Cells(38, 4).Value
        ABWhat17.Value = .Cells(38, 5).Value
'Section 18
        ABRecommend18.Value = .Cells(39, 4).Value
        ABWhat18.Value = .Cells(39, 5).Value
'Section 19
        ABRecommend19.Value = .Cells(40, 4).Value
        ABWhat19.Value = .Cells(40, 5).Value
'Section 20
        ABRecommend20.Value = .Cells(41, 4).Value
        ABWhat20.Value = .Cells(41, 5).Value
'Section 21
        ABRecommend21.Value = .Cells(42, 4).Value
        ABWhat21.Value = .Cells(42, 5).Value
'Section 22
        ABRecommend22.Value = .Cells(43, 4).Value
        ABWhat22.Value = .Cells(43, 5).Value
'Section 23
        ABRecommend23.Value = .Cells(44, 4).Value
        ABWhat23.Value = .Cells(44, 5).Value
'Section 24
        ABRecommend24.Value = .Cells(45, 4).Value
        ABWhat24.Value = .Cells(45, 5).Value
'Section 25
        ABRecommend25.Value = .Cells(46, 4).Value
        ABWhat25.Value = .Cells(46, 5).Value
'Section 26
        ABRecommend26.Value = .Cells(47, 4).Value
        ABWhat26.Value = .Cells(47, 5).Value
        
        'Remainder of the values go here

    End With
'   Enable or disable recommendations and priority if recommend is ticked or not ticked.
' Section 1
    If ABResolve1.Value = False Then
        ABWhat1.Enabled = ABResolve1.Value
        ABRecommend1.Enabled = ABResolve1.Value
        ABRP1.Visible = ABResolve1.Value
        ABRP2.Visible = ABResolve1.Value
        ABRP3.Visible = ABResolve1.Value
        ABRp4.Visible = ABResolve1.Value
        ABRP5.Visible = ABResolve1.Value
        ABPriority1.Visible = ABResolve1.Value
        ABPriorityLabel1.Caption = "Not selected for remediation."
        ABSuccCrit1.Enabled = ABResolve1.Value
    End If
'Section 2
    If ABResolve2.Value = False Then
        ABWhat2.Enabled = ABResolve2.Value
        ABRecommend2.Enabled = ABResolve2.Value
        ABRP6.Visible = ABResolve2.Value
        ABRP7.Visible = ABResolve2.Value
        ABRP8.Visible = ABResolve2.Value
        ABRP9.Visible = ABResolve2.Value
        ABRP10.Visible = ABResolve2.Value
