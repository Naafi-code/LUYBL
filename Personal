            .Cells(1, 391) = ABMetexpect26.Value
            .Cells(1, 392) = ABChangeOcc26.Value
            .Cells(1, 393) = ABAdviserScore26.Value
            .Cells(1, 394) = ABBEScore26.Value
            .Cells(1, 395) = ABTotalScore26.Value
            .Cells(1, 396) = ABToolsText.Value
            .Cells(1, 397) = ABNotesText.Value
            
'Next Section goes here
            
        End With
    
    'ChangeRecord.Max = LUYBLTable.ListRows.Count
    
End Sub
Private Sub PrintButton1_Click()

    'Update the print record to be the current record
        
        Set PRINTTable = Worksheets("PRINT").ListObjects("PRINT")
                
        With PRINTTable
            
        ModifyPrintTableRow PRINTTable.ListRows(1).Range
            
        End With
    
    'Trigger the print dialogue for print to pdf
    
    Dim PrintFile As String
            
    PrintFile = Application.DefaultFilePath & "\" & FirstName.Value & " " & Surname.Value & " " & ActiveWorkbook.Name & ".pdf"
    Sheets("REPORTVIEW").Select
    ActiveSheet.PageSetup.PrintArea = "A1:ET50"
    ActiveSheet.ExportAsFixedFormat Type:=xlTypePDF, Filename:=PrintFile, Quality:=xlQualityStandard, IncludeDocProperties:=True, IgnorePrintAreas:=False, OpenAfterPublish:=True

End Sub

Private Sub AdviserClose_Click()
    
    If MsgBox("This EXIT does NOT SAVE CHANGES MADE to " + RecordPosition.Caption + " Do you wish to PROCEED?", vbYesNo + vbQuestion) = vbNo Then
    
        Exit Sub
    
    End If
    ThisWorkbook.RefreshAll
    
    'Safley terminate the mouse scroll function
    DisableMouseScroll Me
    
    Unload Personal
    Dashboard.Show

End Sub
