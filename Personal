Private Sub UpdateRecordDisplay()

    Set LUYBLTable = Worksheets("LUYBL").ListObjects("LUYBL")

    With LUYBLTable
    
        RecordPosition.Caption = "Record " & CurrentRow & " of " & .ListRows.Count & " records."
       'PopulateForm .ListRows(CurrentRow).Range
 
' Show the loading message form

        LoadData.Show vbModeless
        DoEvents
        
' Load the the data for each record
        
         Section1 .ListRows(CurrentRow).Range
         Section2 .ListRows(CurrentRow).Range
         Section3 .ListRows(CurrentRow).Range
         Section4 .ListRows(CurrentRow).Range
         Section5 .ListRows(CurrentRow).Range
         Section6 .ListRows(CurrentRow).Range
         Section7 .ListRows(CurrentRow).Range
         Section8 .ListRows(CurrentRow).Range
         Section9 .ListRows(CurrentRow).Range
         Section10 .ListRows(CurrentRow).Range
         Section11 .ListRows(CurrentRow).Range
         Section12 .ListRows(CurrentRow).Range
         Section13 .ListRows(CurrentRow).Range
         Section14 .ListRows(CurrentRow).Range
         Section15 .ListRows(CurrentRow).Range
         Section16 .ListRows(CurrentRow).Range
         Section17 .ListRows(CurrentRow).Range
         Section18 .ListRows(CurrentRow).Range
         Section19 .ListRows(CurrentRow).Range
         Section20 .ListRows(CurrentRow).Range
         Section21 .ListRows(CurrentRow).Range
         Section22 .ListRows(CurrentRow).Range
         Section23 .ListRows(CurrentRow).Range
         Section24 .ListRows(CurrentRow).Range
         Section25 .ListRows(CurrentRow).Range
         Section26 .ListRows(CurrentRow).Range

        Unload LoadData

    End With
    
 End Sub

Private Sub ChangeRecord_SpinUp()

    Set LUYBLTable = Worksheets("LUYBL").ListObjects("LUYBL")
  
    If LUYBLTable.ListRows.Count < 1 Then Exit Sub
        
        If CurrentRow > 1 Then
            CurrentRow = CurrentRow - 1
            UpdateRecordDisplay
            
        End If
   
End Sub
Private Sub ChangeRecord_SpinDown()

    Set LUYBLTable = Worksheets("LUYBL").ListObjects("LUYBL")

    If CurrentRow = LUYBLTable.ListRows.Count Then Exit Sub
            
        If CurrentRow < LUYBLTable.ListRows.Count Then
            CurrentRow = CurrentRow + 1
            UpdateRecordDisplay
            
        End If
   
End Sub

Private Sub UpdatePositionCaption()

    Set LUYBLTable = Worksheets("LUYBL").ListObjects("LUYBL")

    RecordPosition.Caption = "Record " & CurrentRow & " of " & LUYBLTable.ListRows.Count & " records."
    
End Sub


Private Sub AdviserDelete_Click()
        
    If MsgBox("This change cannot be undone. You are going to delete record " + RecordPosition.Caption + " records. Do you wish to proceed?", vbYesNo + vbQuestion) = vbNo Then
    
        Exit Sub
    End If
        
        Set LUYBLTable = Worksheets("LUYBL").ListObjects("LUYBL")
        
        If LUYBLTable.ListRows.Count < 1 Then Exit Sub
             
        With Worksheets("LUYBL").ListObjects("LUYBL")
        
            .ListRows(CurrentRow).Delete
            
        If LUYBLTable.ListRows.Count > 0 Then
            
                If CurrentRow > LUYBLTable.ListRows.Count Then
                
                    CurrentRow = LUYBLTable.ListRows.Count
                    
                End If
            Else
            
                CurrentRow = 0
            
            End If
                          
        End With
        
        ChangeRecord.Max = ChangeRecord.Max - 1
        
        UpdatePositionCaption
        
        UpdateRecordDisplay
                
End Sub

Private Sub AdviserInsert_Click()

    'Notify adding a new record
    
    If MsgBox("You are going to insert the displayed data before " + RecordPosition.Caption + " Do you wish to proceed?", vbYesNo + vbQuestion) = vbNo Then
    
    Exit Sub
    End If
    
    Set LUYBLTable = Worksheets("LUYBL").ListObjects("LUYBL")
    
    If CurrentRow = 0 Then CurrentRow = 1
    
    Worksheets("LUYBL").ListObjects("LUYBL").ListRows.Add Position:=CurrentRow
    
    ModifyTableRowAI LUYBLTable.ListRows(CurrentRow).Range
        
    UpdatePositionCaption
    
        MsgBox "Insert successful!"
    
    UpdateRecordDisplay
   
End Sub

Private Sub AdviserUpdate_Click()

    If MsgBox("You are going to update " + RecordPosition.Caption + " Do you wish to proceed?", vbYesNo + vbQuestion) = vbNo Then
        Exit Sub
    End If
  
    Set LUYBLTable = Worksheets("LUYBL").ListObjects("LUYBL")
    
    ModifyTableRow LUYBLTable.ListRows(CurrentRow).Range
   
End Sub

Private Sub AdviserAdd_Click()
    
    If MsgBox("You are going to add a record to " + RecordPosition.Caption + " records. Do you wish to proceed?", vbYesNo + vbQuestion) = vbNo Then
    
        Exit Sub
    
    End If
    
    Set LUYBLTable = Worksheets("LUYBL").ListObjects("LUYBL")
    
    If CurrentRow = 0 Then CurrentRow = 1

    Worksheets("LUYBL").ListObjects("LUYBL").ListRows.Add
     
    ModifyTableRowAI LUYBLTable.ListRows(LUYBLTable.ListRows.Count).Range
    
    UpdatePositionCaption
    
    UpdateRecordDisplay
     
End Sub

Private Sub ModifyTableRowAI(TableRow As Range)
    
    Set LUYBLTable = Worksheets("LUYBL").ListObjects("LUYBL")
    
    With TableRow
    
            .Cells(1, 1) = "Branch..."
            .Cells(1, 2) = "First name..."
            .Cells(1, 3) = "Surname..."
            .Cells(1, 4) = "Cell phone...."
